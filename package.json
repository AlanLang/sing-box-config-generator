{
  "name": "sing-box-config-generator",
  "description": "SingBox 配置文件可视化生成器 - 基于 React + Rust (Axum) 的全栈应用，提供模块化配置管理、订阅管理、备份恢复等功能",
  "version": "1.12.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "author": "Alan",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlanLang/sing-box-config-generator.git"
  },
  "keywords": [
    "singbox",
    "sing-box",
    "proxy",
    "config-generator",
    "react",
    "rust",
    "axum"
  ],
  "scripts": {
    "build": "NODE_ENV=production rsbuild build",
    "check": "biome check --write",
    "type-check": "tsc --noEmit",
    "dev": "rsbuild dev --open && cargo run",
    "format": "biome format --write",
    "preview": "rsbuild preview",
    "prepare": "husky"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.2",
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tabler/icons-react": "^3.35.0",
    "@tanstack/react-query": "^5.90.10",
    "@tanstack/react-router": "^1.139.0",
    "class-variance-authority": "^0.7.1",
    "framer-motion": "^12.31.0",
    "ky": "^1.14.0",
    "lucide-react": "^0.564.0",
    "next-themes": "^0.4.6",
    "radix-ui": "^1.4.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.1",
    "react-resizable-panels": "^4.6.3",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "uuid": "^13.0.0",
    "vaul": "^1.1.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.15",
    "@rsbuild/core": "^1.7.3",
    "@rsbuild/plugin-react": "^1.4.5",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/router-plugin": "^1.159.12",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "bun-types": "^1.3.9",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json}": "biome check --write --no-errors-on-unmatched ."
  }
}